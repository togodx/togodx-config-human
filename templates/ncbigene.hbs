<article data-stanza-database="ncbigene">
  <header>
    <h1>NCBI Gene: {{id}}</h1>
  </header>
  <section>
    <togostanza-hash-table
      data-url="https://integbio.jp/togosite/sparqlist/api/togosite_gene_summary?id={{id}}&amp;type=ncbigene"
      data-type="json"
      format-key="true"
      columns="[{&quot;id&quot;:&quot;NCBI Gene ID&quot;,&quot;link&quot;:&quot;NCBI Gene URL&quot;},{&quot;id&quot;:&quot;Gene symbol&quot;},{&quot;id&quot;:&quot;Description&quot;},{&quot;id&quot;:&quot;Location&quot;},{&quot;id&quot;:&quot;Tissue specificity&quot;},{&quot;id&quot;:&quot;Synonym&quot;},{&quot;id&quot;:&quot;Type&quot;},{&quot;id&quot;:&quot;Other names&quot;}]"
      custom-css-url="/css/stanza.css"
    />
  </section>
</article>
